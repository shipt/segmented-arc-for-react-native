{
  "name": "exampleapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:yarn": "rm -rf node_modules/ && yarn cache clean && yarn install",
    "clean:android": "(cd android && ./gradlew clean && ./gradlew --stop)",
    "clean:ios": "rm -rf ios/build/ && rm -rf ~/library/developer/xcode/deriveddata && yarn pod",
    "clean:cocoapods": "rm -rf ~/.cocoapods/repos",
    "clean:rn": "rm -rf $TMPDIR/react-* && watchman watch-del-all && watchman shutdown-server && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf $TMPDIR/haste-map-react-native-packager-*",
    "clean": "yarn run clean:rn && yarn run clean:yarn && yarn run clean:android && yarn run clean:ios",
    "bundle:install": "(cd ./ios && bundle install) && (cd ./android && bundle install)",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod": "(rm -rf ./ios/Pods && cd ./ios/ && pod install --repo-update)",
    "postinstall": "(cd node_modules/@shipt/segmented-arc-for-react-native && yarn install --production)"
  },
  "dependencies": {
    "@shipt/segmented-arc-for-react-native": "file:../",
    "react": "18.2.0",
    "react-native": "^0.72.12",
    "react-native-svg": ">=12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.12",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
