{
  "name": "@shipt/segmented-arc-for-react-native",
  "version": "1.0.1",
  "description": "Segmented arc component for React Native ",
  "main": "src/index.js",
  "scripts": {
    "clean:yarn": "rm -rf node_modules/ && yarn cache clean && yarn install",
    "clean:android": "(cd example/android && ./gradlew clean && ./gradlew --stop)",
    "clean:ios": "rm -rf example/ios/build/ && rm -rf ~/library/developer/xcode/deriveddata && yarn pod",
    "clean:cocoapods": "rm -rf ~/.cocoapods/repos",
    "clean:rn": "rm -rf $TMPDIR/react-* && watchman watch-del-all && watchman shutdown-server && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf $TMPDIR/haste-map-react-native-packager-*",
    "clean": "yarn run clean:rn && yarn run clean:yarn && yarn run clean:android && yarn run clean:ios",
    "pod": "(rm -rf example/ios/Pods && cd example/ios/ && pod install --repo-update)",
    "bundle:install": "(cd example/ios && bundle install) && (cd example/android && bundle install)",
    "test": "jest",
    "test:o": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "lint": "git diff --name-only --diff-filter=d HEAD | grep -E 'src/.*.js' | grep -v json | xargs node node_modules/eslint/bin/eslint.js --quiet",
    "lint-commit": "git diff --cached --name-only --diff-filter=d HEAD | grep -E 'src/.*.js' | grep -v json | xargs node node_modules/eslint/bin/eslint.js --quiet",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shipt/segmented-arc-for-react-native.git"
  },
  "keywords": [
    "react",
    "react-native",
    "ios",
    "android",
    "segmented-arc",
    "arc",
    "segmented-gauge"
  ],
  "author": "Jakhongir Khusanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shipt/segmented-arc-for-react-native/issues"
  },
  "homepage": "https://github.com/shipt/segmented-arc-for-react-native#readme",
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-native": ">=0.72",
    "react-native-svg": "^12.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.72.2",
    "@react-native/eslint-config": "^0.72.12",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^8.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^7.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.9",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.2",
    "react": "16.x || 17.x",
    "react-native": ">=0.64.2",
    "react-native-svg": "^12.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@react-native|react-native))"
    ]
  }
}
